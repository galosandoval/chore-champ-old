    "@sveltejs/kit": "^1.20.4",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "@types/bcryptjs": "^2.4.2",
    "@types/google.accounts": "^0.0.9",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
